{
  "scripts": {
    "test": "jest",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
