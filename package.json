{
  "scripts": {
    "test": "jest",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*\\.(test|spec)?\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "apollo-server": "^3.12.1",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-express": "^3.12.1",
    "aws-sdk": "^2.1487.0",
    "axios": "^1.6.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
