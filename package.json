      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-express": "^3.12.1",
    "axios": "^1.6.0",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
